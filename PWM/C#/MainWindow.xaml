<Window x:Class="LedOnOffPWM.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LedOnOffPWM"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid Background="#FF23997E">
        <Slider x:Name="sldrOnOff" HorizontalAlignment="Left" Margin="50,142,0,0" VerticalAlignment="Top" Width="400" Maximum="1000" TickFrequency="100" TickPlacement="BottomRight" IsSnapToTickEnabled="True" ValueChanged="sldrOnOff_ValueChanged"/>
        <Label x:Name="lblSlider" Content="0" HorizontalAlignment="Left" Margin="225,20,0,0" VerticalAlignment="Top" FontSize="72"/>

    </Grid>
</Window>
