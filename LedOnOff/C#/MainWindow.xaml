<Window x:Class="LedOnOff.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LedOnOff"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid Background="#FF306280">
        <Button x:Name="btnON" Content="ON" HorizontalAlignment="Left" Margin="38,89,0,0" VerticalAlignment="Top" Width="75" Click="btnON_Click"/>
        <Button x:Name="btnOFF" Content="OFF" HorizontalAlignment="Left" Margin="38,164,0,0" VerticalAlignment="Top" Width="75" Click="btnOFF_Click"/>

    </Grid>
</Window>
